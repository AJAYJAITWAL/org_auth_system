<h3>Members of <%= @organization.name %></h3>
<%= link_to "Add Member", new_organization_membership_path(@organization) %>

<%= link_to "Back to Organization", organization_path(@organization) %>

<ul>
  <% @memberships.each do |membership| %>
    <li><%= membership.user.email %> - <%= membership.role %></li>
  <% end %>
</ul>
